<Window x:Class="Carsharing.CarsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Carsharing"
    mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Carsharing" Height="571" Width="370.667"
        WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded">
    <Grid Background="OldLace" x:Name="mainGrid">
        <ScrollViewer x:Name="mainScrollViewer">
            <StackPanel x:Name="carsStackPanel" Margin="5,5,0,5">
                <TextBlock Text="Here you can rent a car" FontSize="18" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="7"></TextBlock>
                <Button Content="Go to personal page" Style="{StaticResource MaterialDesignFlatAccentBgButton}" Height="28" Width="180" Margin="0, 5, 0, 18" Click="Button_To_Personal_Account_Click"></Button>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
